{
  "name": "angujo/elocrud",
  "description": "Laravel Eloquent CRUD",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "angujo",
      "email": "angujomondi@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "angujo/dbreader": "dev-master",
    "doctrine/inflector": "1.3.*",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "Angujo\\Elocrud\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "H:\\www\\dbreader"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Angujo\\Elocrud\\Laravel\\ElocrudServiceProvider"
      ],
      "aliases": {
        "Elocrud": "Angujo\\Elocrud\\Laravel\\Factory"
      }
    }
  }
}
